{
  "name": "andreacivita/api-crud-generator",
  "description": "Simple API Crud generator for Laravel",
  "type": "library",
  "keywords": [
    "laravel",
    "crud",
    "crud generator",
    "laravel crud generator",
    "api generator"
  ],
  "require": {
    "php": "^7.3|^8.0",
    "laravel/framework": "^8.0",
    "doctrine/dbal": "^2.9",
    "illuminate/support": "^8.61"
  },
  "require-dev": {
    "phpunit/phpunit": "^7|^8",
    "friendsofphp/php-cs-fixer": "^2.16"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Andrea Civita",
      "email": "andreacivita994@gmail.com",
      "homepage": "https://www.andreacivita.it/"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "AndreaCivita\\ApiCrudGenerator\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "AndreaCivita\\ApiCrudGenerator\\ApiCrudGeneratorServiceProvider"
      ]
    }
  }
}
